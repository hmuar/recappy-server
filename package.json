{
  "name": "synapse-hapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "lint": "esw -w src",
    "test-watch": "watch 'npm run test' src",
    "test": "blue-tape -r babel-register src/test/*.test.js | faucet",
    "testo": "clear && blue-tape -r babel-register src/test/controller_pipe_advance_state.test.js",
    "test-dbg-brk":
      "clear && node --inspect-brk $(npm bin)/blue-tape -r babel-register src/test/controller_pipe_advance_state.test.js",
    "test-dbg":
      "clear && node --inspect $(npm bin)/blue-tape -r babel-register src/test/input_evaluation.test.js",
    "docs": "watch 'rm -rf docs && docco src/**/**.js && rm -rf docs/*.test.html' src",
    "sim": "node_modules/.bin/babel-node simulate.js",
    "notify": "NODE_ENV=production node_modules/.bin/babel-node notify.js",
    "notify-dev": "NODE_ENV=dev node_modules/.bin/babel-node notify.js",
    "start": "node register.js",
    "start-dev": "source setup_env.sh; nodemon register.js",
    "start-dev-dbg": "source setup_env.sh; nodemon --inspect register.js",
    "start-clean-dev": "npm run clean-logs && npm run start-dev",
    "clean-logs": "rm -rf logs/*.log",
    "sand": "node_modules/.bin/babel-node src/sandbox.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-root-import": "^4.1.1",
    "bluebird": "^3.4.1",
    "compromise": "^10.2.2",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "good-winston": "^2.1.0",
    "hapi": "^13.5.0",
    "hapi-cors": "^1.0.1",
    "immutable": "^3.8.1",
    "inquirer": "^3.2.3",
    "lodash": "^4.16.4",
    "loggly": "^1.1.0",
    "mongoose": "^4.5.7",
    "natural": "^0.5.1",
    "neo4j-driver": "^1.0.4",
    "node-schedule": "^1.2.4",
    "promised-mongo": "^1.2.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "seraph": "^0.15.2",
    "winston": "^2.3.1",
    "winston-loggly-bulk": "^1.3.3"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-watch": "^2.1.14",
    "faucet": "0.0.1",
    "watch": "^0.19.2",
    "yargs": "^7.0.2"
  }
}
